library(jsonlite)

res <- fromJSON('https://games.crossfit.com/competitions/api/v1/competitions/open/2018/leaderboards?division=1&region=0&scaled=0&sort=0&occupation=0&page=1')
res$pagination$totalCompetitors

res <- fromJSON('https://games.crossfit.com/competitions/api/v1/competitions/open/2018/leaderboards?division=1&region=0&athlete=1309535&scaled=0&sort=0&occupation=0')
res$pagination$totalCompetitors

leaderboard <- res$leaderboardRows
colnames(leaderboard)
typeof(leaderboard)


leaderboard$entrant
colnames(leaderboard$entrant)
typeof(leaderboard$entrant)


leaderboard$entrant$competitorId
leaderboard$entrant$competitorId[1309535]
leaderboard$entrant[leaderboard$entrant$competitorId == 1309535,]

# Get the overall rank

leaderboard[leaderboard$entrant$competitorId == 1309535,]$overallRank



